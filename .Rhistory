#some functions
triang<-function(x, z_min, z_mode, z_max)
for (z_min <= x <= z_mode) {
triang_x <- 2(x-z_min)/(z_max-z_min)*(z_mode-z_min)
}
for (z_mode <= x <= z_max) {
triang_x <- 2(z_max-x)/(z_max-z_min)*(z_max-z_mode)
}
else {
triang_x <- 0
}
return triang_x
